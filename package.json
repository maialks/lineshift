{
  "name": "lineshift",
  "displayName": "LineShift",
  "description": "A extension that improves code navigation by allowing relative line jumps, making development faster and easier. ",
  "icon": "icon.png",
  "version": "0.0.3",
  "publisher": "lkmss",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other",
    "Keymaps"
  ],
  "activationEvents": [
    "onCommand:lineshift.jump"
  ],
  "repository": {
    "url": "https://github.com/maialks/lineshift"
  },
  "license": "MIT",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lineshift.jump",
        "title": "LineShift: Jump to Line"
      }
    ],
    "keybindings": [
      {
        "command": "lineshift.jump",
        "key": "alt+g",
        "when": "editorFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
